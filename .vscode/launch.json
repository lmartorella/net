{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Garden Server",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/target/bin/net8.0/home.garden.exe",
            "args": ["--wrk", "${workspaceFolder}\\target\\etc"],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole",
            "preLaunchTask": "Build: Garden C# server",
            "justMyCode": false
        },
        {
            "name": "Launch Notification Server",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/target/bin/net8.0/home.notification.exe",
            "args": ["--wrk", "${workspaceFolder}\\target\\etc"],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole",
            "preLaunchTask": "Build: Notification server",
            "justMyCode": false
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Web Server",
            "program": "${workspaceFolder}\\src\\web\\server.mjs"
        },
        {
            "name": "Launch Notification Tester",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/mocks/notification-tester/bin/debug/net8.0/notification.tester.exe",
            "args": ["--mqttHost", "192.168.1.40"],
            "stopAtEntry": false,
            "console": "internalConsole",
            "justMyCode": false
        },
    ]
}